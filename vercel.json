{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.ts" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "DATABASE_URL": "process.env.DATABASE_URL",
    "NUVENDE_API_KEY": "process.env.NUVENDE_API_KEY"
  }
}
